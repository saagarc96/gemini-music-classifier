{
  "name": "gemini-music-classifier",
  "version": "1.0.0",
  "description": "Music classification system with Gemini AI and review interface",
  "type": "module",
  "scripts": {
    "dev:client": "cd client && npm run dev",
    "build:client": "cd client && npm run build",
    "dev:setup": "bash scripts/dev-setup.sh",
    "dev:db:start": "docker-compose up -d",
    "dev:db:stop": "docker-compose down",
    "dev:db:reset": "docker-compose down -v && npm run dev:db:start && npm run dev:migrate",
    "dev:db:logs": "docker-compose logs -f postgres",
    "dev:migrate": "dotenv -e .env.local --override -- npx prisma migrate dev",
    "dev:studio": "dotenv -e .env.local --override -- npx prisma studio",
    "dev:seed": "dotenv -e .env.local --override -- npm run enrich:playlist test-data/test-spotify-csv/sample-50-songs.csv --force-duplicates",
    "dev:clone-prod": "bash scripts/clone-production-to-dev.sh",
    "prod:backup": "bash scripts/backup-production.sh",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "create-schema": "prisma migrate deploy",
    "seed": "node scripts/seed.js",
    "enrich:playlist": "node scripts/enrich-playlist.cjs",
    "enrich:spotify": "node scripts/enrich-spotify-playlist.cjs",
    "enrich:batch": "node scripts/batch-enrich-playlists.cjs",
    "process:playlist": "node src/playlist-batch-runner.js",
    "process:all": "node src/orchestrator.js",
    "monitor": "node src/monitor-batch-job.js",
    "merge": "node scripts/merge-results.js",
    "merge:sources": "node scripts/merge-data-sources.cjs",
    "analyze:quality": "node scripts/analyze-data-quality.cjs",
    "check-quota": "node scripts/check-quota.js",
    "generate:constants": "node scripts/generate-frontend-constants.cjs",
    "validate:subgenres": "node scripts/validate-subgenres.cjs",
    "postinstall": "prisma generate"
  },
  "keywords": [
    "music",
    "classification",
    "gemini",
    "batch-api",
    "playlist",
    "review-interface"
  ],
  "author": "Raina Music",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@braintrust/core": "^0.0.17",
    "@google/genai": "^1.3.0",
    "@google/generative-ai": "^0.21.0",
    "@prisma/client": "^6.18.0",
    "@types/formidable": "^3.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@vercel/postgres": "^0.10.0",
    "axios": "^1.13.1",
    "bcryptjs": "^3.0.3",
    "braintrust": "^0.0.170",
    "csv-parse": "^5.6.0",
    "csv-parser": "^3.2.0",
    "csv-stringify": "^6.5.2",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.4.5",
    "fastest-levenshtein": "^1.0.16",
    "formidable": "^3.5.4",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.3",
    "prisma": "^6.18.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "dotenv-cli": "^11.0.0"
  }
}
